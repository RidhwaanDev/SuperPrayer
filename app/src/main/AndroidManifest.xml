<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.home.superprayer">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAqaDB4ocQ36P6xwMLxagGjaSDVPyB21Os" />

        <service android:name=".Network.BackgroundNetwork" />

        <activity
            android:name=".Activity.SettingsActivity"
            android:parentActivityName=".Activity.DashboardActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.DashboardActivity"/>



        </activity>
        <activity android:name=".Activity.Donate" />
        <activity android:name=".Activity.About"
            android:parentActivityName=".Activity.DashboardActivity">

        >

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.DashboardActivity"/>

        </activity>
    </application>

</manifest>